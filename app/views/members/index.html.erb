<h1>Member List</h1><hr>
  
  <%= link_to 'Create Member', new_member_path %>
  <hr>
  
  <% if @members.blank? %>
    <p>Do not have any members.</p>
  <% else %>
    <table>
      <thead>
        <tr>
          <td>Name</td>
          <td>Email</td>
          <td>Contact</td>
          <td>Address</td>
          <td>Action</td>
        </tr>
      </thead>
      <tbody>
        <% @members.each do |member| %>
          <tr>
            <td><%= link_to member.full_name, member_path(member) %></td>
            <td><%= member.email %></td>
            <td><%= member.mobile_number %></td>
            <td><%= member.address %></td>
            <td><%= link_to 'Edit', edit_member_path(member) %> </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <% end %>
  <hr>